com.tongji.demo.name=程序猿DD
com.tongji.demo.title=Spring Boot教程
com.tongji.demo.desc=${com.tongji.demo.name}正在努力写《${com.tongji.demo.title}》

# 随机字符串
com.tongji.demo.value=${random.value}
# 随机int
com.tongji.demo.number=${random.int}
# 随机long
com.tongji.demo.bignumber=${random.long}
# 10以内的随机数
com.tongji.demo.test1=${random.int(10)}
# 10-20的随机数
com.tongji.demo.test2=${random.int[10,20]}

# 多环境配置文件激活属性
spring.profiles.active=dev